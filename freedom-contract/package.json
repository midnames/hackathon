{
  "name": "@meshsdk/freedom-contract",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "compact compile +0.25.0 src/counter.compact src/managed/counter",
    "compact:skip-zk": "compact compile +0.25.0 --skip-zk src/counter.compact src/managed/counter",
    "compact:rebels": "compact compile +0.25.0 src/rebels.compact src/managed/rebels",
    "compact:rebels:skip-zk": "compact compile +0.25.0 --skip-zk src/rebels.compact src/managed/rebels",
    "deploy:rebels": "tsx deploy-rebels.ts",
    "publish:post": "tsx publish-post.ts",
    "test": "vitest run",
    "test:compile": "npm run compact && vitest run",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -Rf ./src/managed ./dist/managed && cp ./src/counter.compact ./src/rebels.compact ./dist",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  }
}
